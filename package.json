{
  "name": "constl",
  "description": "Secure boilerplate for Electron app based on Vite",
  "author": {
    "email": "julien.malard@mail.mcgill.ca",
    "name": "Julien Jean Malard-Adam",
    "url": "https://github.com/julienmalard"
  },
  "version": "0.2.2",
  "main": "packages/main/dist/index.cjs",
  "scripts": {
    "build": "pnpm run build:main && pnpm run build:preload && pnpm run build:renderer",
    "build:main": "cd ./packages/main && vite build",
    "build:preload": "cd ./packages/preload && vite build",
    "build:renderer": "cd ./packages/renderer && vite build",
    "compile": "cross-env MODE=production pnpm run build && electron-builder build --config .electron-builder.config.js --dir --config.asar=false",
    "compile:web": "cross-env MODE=production WEB=true NODE_OPTIONS='--max-old-space-size=8192' pnpm vite build packages/renderer",
    "test": "pnpm run test:main && pnpm run test:preload && pnpm run test:renderer && pnpm run test:e2e",
    "test:e2e": "pnpm run build && vitest run",
    "test:main": "vitest run -r packages/main --passWithNoTests --coverage",
    "test:preload": "vitest run -r packages/preload --passWithNoTests --coverage",
    "test:renderer": "vitest run -r packages/renderer --passWithNoTests --coverage",
    "watch": "node scripts/watch.mjs",
    "watch:web": "cross-env WEB=true vite packages/renderer",
    "preview:web": "pnpm compile:web && cross-env WEB=true vite preview packages/renderer",
    "lint": "eslint . --ext js,mjs,cjs,ts,mts,cts,vue",
    "typecheck:main": "tsc --noEmit -p packages/main/tsconfig.json",
    "typecheck:preload": "tsc --noEmit -p packages/preload/tsconfig.json",
    "typecheck:renderer": "vue-tsc --noEmit -p packages/renderer/tsconfig.json",
    "typecheck": "pnpm run typecheck:main && pnpm run typecheck:preload && pnpm run typecheck:renderer",
    "postinstall": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/update-electron-vendors.mjs",
    "format": "npx prettier --write \"**/*.{js,mjs,cjs,ts,mts,cts,vue,json}\""
  },
  "devDependencies": {
    "@constl/utils-tests": "^0.2.3",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/deep-equal": "^1.0.4",
    "@types/file-saver": "^2.0.7",
    "@types/js-cookie": "^3.0.6",
    "@types/lodash": "^4.14.201",
    "@types/node": "^20.9.0",
    "@types/uuid": "^9.0.7",
    "@types/webfontloader": "^1.6.37",
    "@typescript-eslint/eslint-plugin": "6.9.0",
    "@typescript-eslint/parser": "^6.11.0",
    "@vitejs/plugin-vue": "4.5.1",
    "@vitest/coverage-istanbul": "^0.34.6",
    "@vue/test-utils": "2.4.3",
    "cross-env": "7.0.3",
    "electron": "27.1.3",
    "electron-builder": "24.9.1",
    "eslint": "8.55.0",
    "eslint-plugin-vue": "9.19.2",
    "happy-dom": "12.10.3",
    "nano-staged": "0.8.0",
    "node-pre-gyp": "^0.17.0",
    "playwright": "1.39.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-resolve-id": "^1.0.2",
    "simple-git-hooks": "2.9.0",
    "typescript": "5.2.2",
    "unplugin-auto-expose": "0.0.5",
    "vite": "5.0.4",
    "vite-plugin-commonjs-externals": "^0.1.3",
    "vite-plugin-vuetify": "^1.0.2",
    "vitest": "0.34.6",
    "vue-tsc": "1.8.24"
  },
  "dependencies": {
    "@achingbrain/nat-port-mapper": "1.0.13",
    "@constl/ipa": "0.9.17",
    "@constl/mandataire": "^0.1.15",
    "@constl/mandataire-electron-principal": "^0.2.14",
    "@constl/mandataire-electron-rendu": "^0.2.14",
    "@constl/serveur": "^0.3.5",
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@highlightjs/vue-plugin": "^2.1.0",
    "@lassi-js/kili": "^0.3.4",
    "@lassi-js/kilimukku": "^1.0.3",
    "@lassi-js/kilimukku-vue": "^1.0.11",
    "@mdi/font": "^7.3.67",
    "@vueuse/core": "^10.6.1",
    "assert": "^2.1.0",
    "axios": "^1.6.2",
    "core-js": "^3.33.2",
    "crypto-browserify": "^3.12.0",
    "crypto-js": "^4.2.0",
    "deep-equal": "^2.2.3",
    "electron-updater": "6.1.7",
    "ennikkai": "^2.2.5",
    "events": "^3.3.0",
    "image-file-resize": "^1.0.5",
