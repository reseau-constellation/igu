    "nuchabal": "^3.1.8",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.0",
    "qrcode.vue": "^3.4.1",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-polyfill-node": "^0.12.0",
    "semaphore-async-await": "^1.5.1",
    "tslib": "^2.6.2",
    "uuid": "^9.0.1",
    "vue": "3.3.9",
    "vue-router": "^4.2.5",
    "vuetify": "^3.4.2",
    "webfontloader": "^1.6.28",
    "wherearewe": "^2.0.1",
    "wrtc": "^0.4.7"
  }
}
